# Orchestree Server Configuration

# Server port (default: 3001)
PORT=3001

# API authentication key (leave empty for development mode - no auth required)
ORCHESTREE_API_KEY=

# JWT secret for user authentication (required for email+password auth)
JWT_SECRET=

# CORS allowed origins (comma-separated, default: http://localhost:5173)
CORS_ORIGINS=http://localhost:5173,http://127.0.0.1:5173

# LLM Provider API Keys (set at least one)
ANTHROPIC_API_KEY=
OPENAI_API_KEY=
GOOGLE_API_KEY=
DEEPSEEK_API_KEY=
MISTRAL_API_KEY=
GROQ_API_KEY=
OPENROUTER_API_KEY=
XAI_API_KEY=
MINIMAX_API_KEY=

# Logging
LOG_LEVEL=info
